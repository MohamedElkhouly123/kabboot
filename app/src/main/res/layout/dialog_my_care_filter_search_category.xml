<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/_20sdp"
    android:layout_marginBottom="@dimen/_20sdp"
    android:layout_gravity="center"
    >
    <!--    app:cardCornerRadius="@dimen/_10sdp"-->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="@dimen/_5sdp"
        android:layout_margin="@dimen/_8sdp"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/dialog_flight_search_category_close_btn"
            android:layout_width="@dimen/_15sdp"
            android:layout_height="@dimen/_15sdp"
            android:layout_gravity="right"
            android:layout_marginEnd="@dimen/_1sdp"
            android:layout_marginTop="@dimen/_1sdp"
            android:background="@drawable/red_shape_circle"
            android:elevation="@dimen/_3sdp"
            android:padding="@dimen/_1sdp"
            android:scaleType="centerCrop"
            android:src="@drawable/ic_clear_black_24dp"
            />
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fillViewport="true"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                >
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:elevation="3dp"
                    android:textStyle="bold"
                    android:textSize="@dimen/_18sdp"
                    android:textColor="@color/black2"
                    android:text="@string/my_care"
                    />
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/fragment_login_til_make"
                    style="@style/text_input_login"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_20sdp"
                    app:errorTextAppearance="@style/error_appearance"
                    >

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/fragment_login_make_etxt"
                        style="@style/EditTextLogin"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_56sdp"
                        android:hint="@string/make"
                        android:drawableStart="@drawable/ic_baseline_directions_car_24"
                        android:drawablePadding="@dimen/_10sdp"
                        android:ems="10"
                        android:inputType="textAutoComplete" />
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/fragment_login_til_model"
                    style="@style/text_input_login"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_20sdp"
                    app:errorTextAppearance="@style/error_appearance"
                    >

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/fragment_login_model_etxt"
                        style="@style/EditTextLogin"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_56sdp"
                        android:hint="@string/model"
                        android:drawableStart="@drawable/ic_baseline_directions_car_24"
                        android:drawablePadding="@dimen/_10sdp"
                        android:ems="10"
                        android:inputType="textAutoComplete" />
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/fragment_login_til_year"
                    style="@style/text_input_login"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_20sdp"
                    app:errorTextAppearance="@style/error_appearance"
                    >

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/fragment_login_year_etxt"
                        style="@style/EditTextLogin"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_56sdp"
                        android:hint="@string/year"
                        android:ems="10"
                        android:inputType="number" />
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/fragment_login_til_liters"
                    style="@style/text_input_login"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_20sdp"
                    app:errorTextAppearance="@style/error_appearance"
                    >

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/fragment_login_liters_etxt"
                        style="@style/EditTextLogin"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_56sdp"
                        android:hint="@string/liters"
                        android:ems="10"
                        android:inputType="number" />
                </com.google.android.material.textfield.TextInputLayout>
